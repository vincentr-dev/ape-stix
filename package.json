{
  "name": "ape",
  "version": "0.0.1",
  "description": "front-end boilerplate using webpack",
  "private:": "true",
  "main": "index.ts",
  "scripts": {
    "build": "npx webpack-cli --mode development",
    "watch": "npx webpack-cli --watch --mode development",
    "server": "npx ts-node --project ./server/tsconfig.json server/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx concurrently --kill-others \"npm:server\" \"npm:watch\""
  },
  "keywords": [
    "webpack",
    "front-end",
    "frontend",
    "typescript",
    "css",
    "html"
  ],
  "author": "vincent.v.romero@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/html-webpack-plugin": "^3.2.3",
    "concurrently": "^5.2.0",
    "css-loader": "^3.6.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "sass": "^1.26.9",
    "sass-loader": "^9.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "jquery": "^3.5.1"
  }
}
